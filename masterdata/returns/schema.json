{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Return",
  "properties": {
    "idOrden": {
      "description": "Return name",
      "type": "string"
    },
    "itemsDevolucion": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "properties": {
          "imagen": {
            "description": "Return imagen devolucion",
            "format": "uri",
            "type": "string"
          },
          "id": {
            "description": "Return id devolucion",
            "type": "string"
          },
          "quantity": {
            "description": "Return cantidad productos",
            "type": "integer"
          },
          "name": {
            "description": "Return nombre producto",
            "type": "string"
          },
          "price": {
            "description": "Return precio producto",
            "type": "integer"
          }
        },
        "required": ["id", "quantity", "name", "price"]
        
      }
    },
    "metodoDevolucionDinero": {
      "description": "Return metodo devolucion",
      "type": "string"
    },
    "usuario_correo": {
      "description": "Return correos usuario",
      "format": "email",
      "type": "string"
    }
  },
  "required": [
    "idOrden",    
    "metodoDevolucionDinero",
    "usuario_correo"
  ],
  "v-cache": false,
  "v-indexed": ["idOrden","usuario_correo","itemsDevolucion.id"],
  "v-immediate-indexing": true,
  "v-security": {
    "allowGetAll": true
  }
}
